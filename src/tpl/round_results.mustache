{{{html_header}}}
<!-- {{generated_by}} -->
<table {{html_table_properties}}>
    <tr {{html_table_headline_properties}}>
        <td colspan='3'>{{label_round}} {{round_number}}</td>
        <td colspan='3'>{{label_victorypoints}}</td>
        <td>&nbsp;</td>
        <td colspan='3'>{{label_points}}</td>
        <td>{{label_result}}</td>
    </tr>
{{#results}}
    <tr>
        <td>{{player1_name}}</td>
        <td>{{vs}}&nbsp;</td>
        <td>{{player2_name}}</td>
        <td>{{player1_victorypoints}}&nbsp;</td>
        <td>{{separator}}&nbsp;</td>
        <td>{{player2_victorypoints}}&nbsp;</td>
        <td>&nbsp;</td>
        <td>{{player1_points}}&nbsp;</td>
        <td>{{separator}}&nbsp;</td>
        <td>{{player2_points}}&nbsp;</td>
        <td>{{result}}&nbsp;</td>
    </tr>
{{/results}}
</table>
{{{html_footer}}}